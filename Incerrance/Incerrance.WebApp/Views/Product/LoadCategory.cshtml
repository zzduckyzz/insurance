@model IEnumerable<Incerrance.Model.DAL.ProductCategory>
@if ((int)ViewBag.Count > 0)
{
    <li><a href="/tat-ca-san-pham">Tất cả sản phẩm <span class="count">(@ViewBag.CountAllProduct)</span></a></li>
    foreach (var item in Model)
    {
        var url = "/loai-san-pham/" + item.MetaTitle + "/" + item.Id;
        <li><a href="@url">@item.Name</a> <span class="count">(@item.Product.Count)</span></li>
    }
}

