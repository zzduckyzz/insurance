@model IEnumerable<Incerrance.Model.DAL.InsurranceCategory>
@if ((int)ViewBag.Count > 0)
{
	<li><a href="/tat-ca-san-pham">All insurance <span class="count">(@ViewBag.CountAllInsurrance)</span></a></li>
	foreach (var item in Model)
	{
		var url = "/loai-san-pham/" + item.MetaTitle + "/" + item.Id;
		<li><a href="@url">@item.Name</a> <span class="count">(@item.Insurrances.Where(s=>s.IsDeleted == false).Count())</span></li>
	}
}

