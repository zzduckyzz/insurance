@model IEnumerable<Incerrance.Model.DAL.Insurrance>
@if ((int)ViewBag.Count > 0)
{
	foreach (var item in Model)
	{
		var url = "/insurrance/" + item.MetaTitle + "/" + item.Id;
		<div class="col-md-3 product-left">
			<div class="p-one simpleCart_shelfItem" style="height:410px;">
				<a href="@url">
					<img src="@item.ImagesInsurrance" width="223" height="232" alt="" />
					@*<div class="mask">
							<span>See details</span>
						</div>*@
				</a>
				<h4>@item.Name</h4>
				<h5>@Convert.ToDecimal(item.Price).ToString("#,##0") VNĐ</h5>
				<h5>@item.InsurancePeriod Month</h5>
				<h5>@item.InsurranceCategory.Name</h5>
			</div>
		</div>
	}
}