@model IEnumerable<Incerrance.Model.DAL.Contact>
@{
    int i = 0;
}

<div class="footer" id="Contact">
    <div class="container">
        <div class="footer-top">
            @if (ViewBag.Count > 0)
            {
                foreach (var item in Model)
                {
                    <div class="col-md-4 footer-left">
                        <h3>@item.Title</h3>
                        <ul>
                            <li><b>@item.ObjectName</b></li>
							<li>Address: <a href="#">@item.Address</a></li>
							<li>Phone number: <a href="#">@item.Mobile</a></li>
                            <li>Website: <a href="#">@item.Website</a></li>
                            <li>Email: <a href="#">@item.Email</a></li>
                        </ul>
                    </div>
                }
                <div class="col-md-4 footer-left">
					<h3>Fanpage</h3>
                    
                </div>
            }
            <div class="clearfix"> </div>
        </div>
    </div>
</div>