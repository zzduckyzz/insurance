@model ShoeShopOnline.Model.DAL.NewsCategory

@{
    Layout = null;
}
<div class="modal fade" id="MyModal">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Chi tiết loại tin tức</h4>
            </div>
            <div class="modal-body">
                <table class="table table-bordered table-striped">
                    <tr>
                        <th>@Html.DisplayNameFor(model => model.Name)</th>
                        <td>@Html.DisplayFor(model => model.Name)</td>
                    </tr>
                    <tr>
                        <th>@Html.DisplayNameFor(model => model.MetaTitle)</th>
                        <td>@Html.DisplayFor(model => model.MetaTitle)</td>
                    </tr>
                    <tr>
                        <th>@Html.DisplayNameFor(model => model.DisplayOrder)</th>
                        <td>@Html.DisplayFor(model => model.DisplayOrder)</td>
                    </tr>
                    <tr>
                        <th>@Html.DisplayNameFor(model => model.SeoTitle)</th>
                        <td>@Html.DisplayFor(model => model.SeoTitle)</td>
                    </tr>
                    <tr>
                        <th>@Html.DisplayNameFor(model => model.MetaKeywords)</th>
                        <td>@Html.DisplayFor(model => model.MetaKeywords)</td>
                    </tr>
                    <tr>
                        <th>@Html.DisplayNameFor(model => model.CreatedBy)</th>
                        <td>@Html.DisplayFor(model => model.CreatedBy)</td>
                    </tr>
                    <tr>
                        <th>@Html.DisplayNameFor(model => model.CreatedOn)</th>
                        <td>@Html.DisplayFor(model => model.CreatedOn)</td>
                    </tr>
                    <tr>
                        <th>@Html.DisplayNameFor(model => model.ModifiedBy)</th>
                        <td>@Html.DisplayFor(model => model.ModifiedBy)</td>
                    </tr>
                    <tr>
                        <th>@Html.DisplayNameFor(model => model.ModifiedOn)</th>
                        <td>@Html.DisplayFor(model => model.ModifiedOn)</td>
                    </tr>
                    <tr>
                        <th>@Html.DisplayNameFor(model => model.IsDeleted)</th>
                        <td>@Html.DisplayFor(model => model.IsDeleted)</td>
                    </tr>
                </table>
            </div>
            <!-- /.modal-body -->
            <div class="modal-footer">
                <div class="form-actions no-color text-center">
                    <a href="/quan-tri/loai-san-pham/cap-nhat?id=@Model.Id" class="btn-sm btn-group-sm btn-primary"><i class="far fa-edit"></i>Sửa</a>
                    <button type="button" class="btn btn-sm btn-primary" style=" border: 0.5px solid #a8a9ad;" data-dismiss="modal">Thoát</button>
                </div>
            </div>
            <!-- /.modal-footer -->
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
<!-- /.modal -->