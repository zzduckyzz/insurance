@model IEnumerable<ShoeShopOnline.Model.DAL.Contact>
@{
    int i = 0;
}

<div class="footer" id="Contact">
    <div class="container">
        <div class="footer-top">
            @if (ViewBag.Count > 0)
            {
                foreach (var item in Model)
                {
                    <div class="col-md-4 footer-left">
                        <h3>@item.Title</h3>
                        <ul>
                            <li><b>@item.ObjectName</b></li>
                            <li>Địa chỉ: <a href="#">@item.Address</a></li>
                            <li>Sđt: <a href="#">@item.Mobile</a></li>
                            <li>Website: <a href="#">@item.Website</a></li>
                            <li>Email: <a href="#">@item.Email</a></li>
                        </ul>
                    </div>
                }
                <div class="col-md-4 footer-left">
                    <h3>Kết nối mạng xã hội</h3>
                    <ul>
                        <iframe src="https://www.facebook.com/plugins/page.php?href=https%3a%2f%2fwww.facebook.com%2ffreestyledn%2f&tabs=timeline&width=340&height=200&small_header=false&adapt_container_width=true&hide_cover=false&show_facepile=true&appid=1280279928786473" width="340" height="200" style="border:none;overflow:hidden" scrolling="no" frameborder="0" allowtransparency="true" allow="encrypted-media"></iframe>
                    </ul>
                </div>
            }
            <div class="clearfix"> </div>
        </div>
    </div>
</div>