@model IEnumerable<ShoeShopOnline.Model.DAL.NewsCategory>
@if ((int)ViewBag.Count > 0)
{
    <li><a href="/tin-tuc">Tất cả tin tức</a></li>
    foreach (var item in Model)
    {
        var url = "/loai-tin-tuc/" + item.MetaTitle + "/" + item.Id;
        <li><a href="@url">@item.Name</a> <span class="count">(@item.News.Count)</span></li>
    }
}

