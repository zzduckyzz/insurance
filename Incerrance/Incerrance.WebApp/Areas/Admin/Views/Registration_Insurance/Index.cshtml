@model IEnumerable<Incerrance.Model.DAL.Registration_Insurance>

@{
	ViewBag.Title = "Registration Insurance List";
	Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
	int i = 1;
}
<div class="card shadow mb-4">
	<div class="card-header py-3">
		<h6 class="m-0 font-weight-bold text-primary">@ViewBag.Title</h6>
	</div>
	<div class="card-body">
		<div class="table-responsive">
			<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
				<thead>
					<tr>
						<th>Stt</th>
						<th>
							@Html.DisplayNameFor(model => model.Insurrance.InsurranceCode)
						</th>
						<th>
							@Html.DisplayNameFor(model => model.FullName)
						</th>
						<th>
							@Html.DisplayNameFor(model => model.Email)
						</th>
						<th>
							@Html.DisplayNameFor(model => model.LicensePlate)
						</th>
						<th>
							@Html.DisplayNameFor(model => model.VehicleRate)
						</th>
						<th>
							@Html.DisplayNameFor(model => model.RegistrationDate)
						</th>
						<th>
							@Html.DisplayNameFor(model => model.ExpirationDate)
						</th>
						<th>
							@Html.DisplayNameFor(model => model.TotalMoney) VNĐ
						</th>
						<th>
							@Html.DisplayNameFor(model => model.CMND)
						</th>
						<th>
							@Html.DisplayNameFor(model => model.Status)
						</th>
						<th></th>
					</tr>
				</thead>
				<tbody>
					@foreach (var item in Model)
					{
						<tr>
							<td>@i</td>
							<td>
								@Html.DisplayFor(modelItem => item.Insurrance.Name)
							</td>

							<td>
								@Html.DisplayFor(modelItem => item.FullName)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.Email)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.LicensePlate)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.VehicleRate)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.RegistrationDate)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.ExpirationDate)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.TotalMoney)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.CMND)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.Status)
							</td>
							<td>
								<a class="btn-sm btn-group-sm btn-success" href="/Admin/Registration_Insurance/Edit?id=@item.Id"><i class="far fa-edit"></i></a>
								<a class="btn-sm btn-group-sm btn-secondary dialog-link" href="@Url.Action("Details", new { id = item.Id })"><i class="fas fa-info"></i></a>
							</td>
						</tr>
						i++;
					}
				</tbody>
			</table>
			@if (Model.Count() <= 0)
			{
				<div class="text-center text-danger">No data</div>
			}
		</div>
	</div>
</div>


		
